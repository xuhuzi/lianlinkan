package cn.elva.model;
public class ArrayPoint{
		protected int i; //表示行
		protected int j; //表示列
		protected int value; //表示值
	public ArrayPoint( int i,int j,int value ){
		this.i =i;
		this.j =j;
		this.value = value;
	}
	public String toString( ){
		return i+","+j+","+value;
	}
	public int getI(){
		return i;
	}
	public void setI(int i){
		this.i = i;
	}
	public int getJ(){
		return j;
	}
	public void setJ(int j){
		this.j = j;
	}
	public int getValue(){
		return value;
	}
